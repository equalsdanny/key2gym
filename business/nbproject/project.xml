<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.ant.freeform</type>
    <configuration>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <!-- Do not use Project Properties customizer when editing this file manually. -->
            <name>Key2Gym-Business</name>
            <properties>
                <property-file>../private/environment.properties</property-file>
                <property-file>project.properties</property-file>
            </properties>
            <folders>
                <source-folder>
                    <label>Source</label>
                    <type>java</type>
                    <location>${src.dir}</location>
                </source-folder>
            </folders>
            <ide-actions>
                <action name="build">
                    <target>build</target>
                </action>
                <action name="clean">
                    <target>clean</target>
                </action>
                <action name="rebuild">
                    <target>clean</target>
                    <target>build</target>
                </action>
                <action name="rebuild-deploy">
                    <target>clean</target>
                    <target>build</target>
                    <target>deploy</target>
                </action>
            </ide-actions>
            <view>
                <items>
                    <source-folder style="packages">
                        <label>Source</label>
                        <location>${src.dir}</location>
                    </source-folder>
                    <source-file>
                        <location>build.xml</location>
                    </source-file>
                    <source-file>
                        <location>project.properties</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="build"/>
                    <ide-action name="rebuild"/>
                    <ide-action name="clean"/>
                    <ide-action name="rebuild-deploy" />
                </context-menu>
            </view>
            <subprojects/>
        </general-data>
        <java-data xmlns="http://www.netbeans.org/ns/freeform-project-java/1">
            <compilation-unit>
                <package-root>${src.dir}/</package-root>
                <classpath mode="compile">${javac.classpath}</classpath>
                <source-level>1.5</source-level>
            </compilation-unit>
        </java-data>
    </configuration>
</project>
