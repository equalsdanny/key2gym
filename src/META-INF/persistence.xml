<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="Census PU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>census.persistence.Key</class>
    <class>census.persistence.Item</class>
    <class>census.persistence.ItemSubscription</class>
    <class>census.persistence.TimeRange</class>
    <class>census.persistence.ClientProfile</class>
    <class>census.persistence.Client</class>
    <class>census.persistence.AdSource</class>
    <class>census.persistence.Attendance</class>
    <class>census.persistence.OrderEntity</class>
    <class>census.persistence.OrderLine</class>
    <class>census.persistence.Administrator</class>
    <class>census.persistence.Session</class>
    <class>census.persistence.ClientFreeze</class>
    <class>census.persistence.Property</class>
    <class>census.persistence.CashAdjustment</class>
    <properties>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="eclipselink.logging.logger" value="org.eclipse.persistence.logging.CommonsLoggingSessionLog"/>
      <property name="eclipselink.logging.level" value="CONFIG"/>
    </properties>
  </persistence-unit>
</persistence>
